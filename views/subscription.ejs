<%- include('header', { sources }) %>
        <!-- 右侧 -->
        <div id="right">
            <div id="swith_one">
                <!-- 左侧菜单折叠展开按钮 -->
                <img src="/img/swith_one.jpg">
            </div>
            <div id="head">
                <form action="#" method="post">
                      <input class="ser" type="text"><input class="btn_ser" type="submit" value="搜索"><!-- 搜索框-->
                </form>
                <!-- 搜索框-->
                <div id="head_sculpture">
                    <strng><%= username %></strng>
                    <img src="/img/icon2.png">
                </div>
                <!-- 头像    改为用户名-->
            </div>
            <div id="cut_line"></div>
            <div id="right_body">
                <div id="manage_site">
                    <div id="all_head">
                        <div class="a">
                            <strong>订阅管理</strong>
                            <br>
                            <span>已订阅：<%= sources.length %></span>
                        </div>
                    </div>
                    <!--<div class="b">刷新</div>-->
                    <br>
                    <div id="manage">
                        <div id="url_serch">
                            <div>
                                <input type="text" name = "rss" placeholder="输入网站链接">
                            </div>
                            <div>
                                <img src="/img/icon6.jpg">
                            </div>
                        </div>
                        <div id="publicPlatform">
                            <% sources.forEach(function(source){ %>
                            <div>
                                <span><%= sources.name %></span>
                                <img src="/img/icon5.jpg">
                            </div>
                            <% }); %>

                        </div>
                    </div>

                </div>
            </div>

        </div>
<%- include('footer') %>
